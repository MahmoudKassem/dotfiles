pcm.!default {
    type plug
    slave.pcm "jack"
}

pcm.!jack {
    type jack
    playback_ports {
        0 system:playback_1
        1 system:playback_2
        2 system:playback_3
        3 system:playback_4
    }
    capture_ports {
        0 system:capture_1
        1 system:capture_2
    }
}

ctl.!default {
    type hw
    card Generic_1
}
